<h1>Artists#new</h1>
<p>Find me in app/views/artists/new.html.erb</p>

<%= form_for @artist do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, :autofocus => true %>

  <%= f.label :nationality %>
  <%= f.text_field :nationality %>

  <%= f.label :dob, "Date of birth" %>
  <%= f.text_field :dob %>

  <%= f.label :period %>
  <%= f.text_field :period %>

  <%= f.label :image %>
  <%= f.url_field :image %>

  <%= f.submit %>
<% end %>
